{
  "pipeline": [
    "/assets/ept-data/*.laz",
    {
      "type": "filters.reprojection",
      "in_srs": "EPSG:32722",
      "out_srs": "EPSG:4978"
    },
    {
      "type": "filters.splitter",
      "length": 100
    },
    {
      "type": "writers.las",
      "filename": "tilesLAZ/tile_#.laz"
    }
  ]
}
